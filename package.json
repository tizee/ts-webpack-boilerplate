{
  "name": "ts-webpack-boilerplate",
  "version": "0.0.1",
  "description": "A simple Typescript bolierplate with boilerplate",
  "author": "tizee",
  "license": "MIT",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "ts-check:watch": "tsc --noEmit --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint src",
    "format": "prettier src/**/*.{js,ts,jsx,tsx} --write",
    "dev": "cross-env webpack --config webpack.dev.js",
    "build": "cross-env webpack --config webpack.prod.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "ts-loader": "^8.1.0",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
